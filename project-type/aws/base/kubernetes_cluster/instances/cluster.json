{
  "flavor": "eks_standard",
  "metadata": {
    "name": "eks-cluster"
  },
  "kind": "kubernetes_cluster",
  "inputs": {
    "cloud_account": {
      "resource_name": "cloud",
      "resource_type": "cloud_account"
    },
    "network_details": {
      "resource_name": "network",
      "resource_type": "network"
    }
  },
  "disabled": false,
  "version": "1.0",
  "spec": {
    "cluster_endpoint_private_access": true,
    "cluster_endpoint_public_access": true,
    "cluster_version": "1.34",
    "enable_cluster_encryption": false,
    "cluster_addons": {
      "additional_addons": {},
      "coredns": {
        "enabled": true,
        "version": "latest"
      },
      "kube_proxy": {
        "enabled": true,
        "version": "latest"
      },
      "vpc_cni": {
        "enabled": true,
        "version": "latest"
      }
    },
    "default_node_pool": {
      "capacity_type": "SPOT",
      "desired_size": 2,
      "disk_size": 50,
      "instance_types": ["t3.medium"],
      "max_size": 5,
      "min_size": 2
    },
    "cluster_tags": {}
  },
  "uiProps": {}
}
