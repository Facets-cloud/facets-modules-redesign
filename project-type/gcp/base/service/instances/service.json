{
  "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/service/service.schema.json",
  "flavor": "gcp",
  "metadata": {
    "labels": {
      "deliveryType": "MANUAL"
    }
  },
  "kind": "service",
  "disabled": true,
  "version": "1.0",
  "spec": {
    "type": "application",
    "enable_host_anti_affinity": false,
    "restart_policy": "Always",
    "release": {
      "strategy": {
        "type": "RollingUpdate"
      }
    },
    "cloud_permissions": {
      "gcp": {
        "roles": {}
      }
    },
    "runtime": {
      "args": [],
      "command": [],
      "autoscaling": {
        "scaling_on": "CPU",
        "min": 1,
        "max": 1,
        "cpu_threshold": "50"
      },
      "ports": {},
      "size": {
        "cpu": "300m",
        "memory": "1Gi",
        "cpu_limit": "1000m",
        "memory_limit": "5Gi"
      },
      "health_checks": {
        "readiness_check_type": "None",
        "liveness_check_type": "None"
      }
    },
    "env": {
      "LOG_LEVEL": "INFO"
    },
    "init_containers": {},
    "sidecars": {}
  },
  "inputs": {
    "kubernetes_details": {
      "resource_name": "cluster",
      "resource_type": "kubernetes_cluster",
      "output_name": "attributes"
    },
    "cloud_account": {
      "resource_name": "cloud",
      "resource_type": "cloud_account"
    },
    "artifactories": {
      "resource_name": "artifactories",
      "resource_type": "artifactories"
    },
    "vpa_details": {
      "resource_name": "vpa",
      "resource_type": "vpa"
    },
    "kubernetes_node_pool_details": {
      "resource_name": "nodepool",
      "resource_type": "kubernetes_node_pool",
      "output_name": "attributes"
    }
  }
}